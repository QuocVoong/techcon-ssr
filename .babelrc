{
  "presets": [
    "react",
    [
      "env",
      {
        "targets": {
          "browsers": ["last 2 versions", "IE >= 8"],
          "node": "current"
        },
        "useBuiltIns": "entry"
      }
    ],
    "stage-0"
  ],
  "plugins": [
    "transform-decorators-legacy",
    "react-loadable/babel",
    "react-hot-loader/babel"
  ],
  "env": {
    "development": {
      "plugins": [
        "transform-regenerator"
      ]
    }
  }
}
